if (!parameters_interface->has_parameter("{{parameter_name}}")) {
    rcl_interfaces::msg::ParameterDescriptor descriptor;
    descriptor.description = "{{parameter_description}}";
    descriptor.read_only = {{parameter_read_only}};
    auto parameter = rclcpp::ParameterType::{{parameter_type}};
    parameters_interface->declare_parameter("{{parameter_name}}", parameter, descriptor);
}
